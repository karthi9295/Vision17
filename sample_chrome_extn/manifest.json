{
  "manifest_version": 2,

  "name": "Data Scrapper",
  "description": "This extension is used for extracting the data from the website",
  "version": "1.0",

  "browser_action": {
    "default_icon": "download.png",
    "default_popup": "popup.html"
  },

  "icons" : {
	"64" : "download.png"
  },
    "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["jquery_edited_new1.js"]
    }
  ]
}
